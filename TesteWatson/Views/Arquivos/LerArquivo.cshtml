@model IEnumerable<TesteWatson.Models.RetornoApiPerfil>
<html>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<h2>Basic File Upload</h2>
@using (Html.BeginForm("LerArquivo",
                                    "Arquivos",
                                    FormMethod.Post,
                                    new { enctype = "multipart/form-data" }))
{
    <label for="file">Novo documento:</label>
    <input type="file" name="file" id="file" />
    <br>
    <br>
    <input type="submit" value="Upload document" />
    <br>
    <br>

    <pre>
        @ViewBag.resultado
    </pre>
}  

</html>
