@model IEnumerable<TesteWatson.Models.arquivobase>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div class="editor-field">
    @Html.DropDownList("aluno")
   
</div>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.arquivo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.aluno.matricula)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.aluno.nome)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.arquivo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.aluno.matricula)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.aluno.nome)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
